function backup {
    rank=$2
    rank=${rank:-$RANK}
    check_root
    check_rm_rank_lo
    backup_dir=$BACKUP_DIR/$rank
    backup_file=$backup_dir/libreosteo-$(date +%Y-%m-%d-%H%M%S).tar.gz
    lo_rank_dir=$LO_DIR/$rank

    mkdir -p $backup_dir

    cd $lo_rank_dir
    docker-compose down
    tar cvzf $backup_file .
    docker-compose up -d
}

